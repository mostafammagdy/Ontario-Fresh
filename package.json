{
  "name": "code",
  "version": "2.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "custom-react-scripts": "^0.2.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "moment": "^2.19.3",
    "prop-types": "^15.5.10",
    "react-scripts": "^4.0.1",
    "sass": "^1.35.2"
  },
  "dependencies": {
    "@sentry/browser": "^4.2.3",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.16.2",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.7.0",
    "cropperjs": "^1.5.11",
    "d3": "^4.13.0",
    "flat": "^4.0.0",
    "jsonp": "^0.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.1",
    "react": "^17.0.1",
    "react-accessible-accordion": "^3.3.4",
    "react-autosuggest": "^9.3.4",
    "react-clamp-lines": "^1.1.0",
    "react-cropper": "^1.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^4.2.1",
    "react-flexbox-grid": "^1.1.5",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-image-crop": "^8.6.6",
    "react-redux": "^5.0.2",
    "react-responsive": "^1.3.4",
    "react-router": "^3.0.2",
    "react-themeable": "^1.1.0",
    "recompose": "^0.25.1",
    "redux": "^3.6.0",
    "redux-form": "^7.2.0",
    "redux-form-material-ui": "^4.2.0",
    "redux-saga": "^0.16.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "localStorage": true,
      "document": true,
      "fetch": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "React"
        }
      ],
      "no-shadow": 0,
      "react/jsx-uses-vars": [
        2
      ],
      "react/require-default-props": [
        0
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
